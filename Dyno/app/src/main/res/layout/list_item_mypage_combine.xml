<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/item_combine_background"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="70dp"
    android:orientation="horizontal">
    <LinearLayout
        android:layout_width="0dp"
        android:layout_weight="3"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:layout_marginStart="30dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/mypage_combine_start"
            android:gravity="center_vertical"
            android:textSize="20sp"
            android:textColor="@color/dynoBlack" />
        <TextView
            android:id="@+id/item_combine_name1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/mypage_combine_m1"
            android:gravity="center_vertical"
            android:textSize="20sp"
            android:textColor="@color/dynoBlack" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/mypage_combine_plus"
            android:gravity="center_vertical"
            android:textSize="20sp"
            android:textColor="@color/dynoBlack" />
        <TextView
            android:id="@+id/item_combine_name2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/mypage_combine_s1"
            android:gravity="center_vertical"
            android:textSize="20sp"
            android:textColor="@color/dynoBlack" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/mypage_combine_end"
            android:gravity="center_vertical"
            android:textSize="20sp"
            android:textColor="@color/dynoBlack" />

    </LinearLayout>


    <TextView
        android:layout_width="0dp"
        android:layout_weight="2"
        android:layout_height="match_parent"
        android:text="@string/mypage_date"
        android:gravity="center_vertical"
        android:textSize="15sp"
        android:textColor="@color/dynoBlack"/>

</LinearLayout>